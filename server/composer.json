{
    "name": "cinema/php-backend",
    "description": "Cinema Management API with PHP",
    "type": "project",
    "require": {
      "php": "^8.0",
      "ext-pdo": "*",
      "vlucas/phpdotenv": "^5.5",
      "illuminate/database": "^10.43"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
  },
  "minimum-stability": "stable"
  }